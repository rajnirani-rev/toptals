}
}

@media(max-width: 1024px) {
/*plan page css*/
	.plan-top-bar {
		background-image: url("../images/plan-page-top-arrow-background.png");
		background-position: right 0 top;
		background-repeat: no-repeat;
		background-size: 36% auto;
	}
	.cycle-sub-block:first-child {
    margin-left: 0;
}
	.footer-bottom-plan-back {
    padding-top: 190px;
	}
	.border-circle .cycle-sub-block {
margin-right: 0px;
	}
	.top-bottom-arrow {
		background-position: left 40px top;
	}
	.plan-wrapper {
    margin: 0 auto;
}
}
@media(max-width:1023px)
{
	.top-bottom-arrow {
    	background-image: none !important;
	 }
	.cycle-blocks {
    	background-image: none !important;
	}
	.plan-top-bar {
		background-image: none !important;
	}

	.prize-wrapper {
    margin: 0 0 0 auto;
    padding-left: 0;
}
	.cycle-sub-block {
    width: 229px;
	height: 229px;
	}
	.border-circle {
    margin-top: 0px;
    padding-left: 0px;
	}
	.cycle-sub-block .inner-num-text {
    margin-bottom: 0;
    margin-top: 0;
    padding: 40% 29%;
	}
	.plan-bottom-arrow {
		background-image: none !important;
		display: none;
 }
@media(max-width:991px) {
	/*plan page css*/
	
	.top-arrow-title {
    	padding-top: 2pc;
	}
	
	.border-circle {
    width: 68%;
	}
	.cycle-sub-block:first-child {
    margin-left: -3px;
}
	/*plan page css ends*/
}
@media(max-width:980px){
/*plan css*/
.border-circle {
	   padding-left: 26px;
    }
.top-arrow-title {
    padding-top: 2pc;
    }

.prize-wrapper {
    margin-bottom: 0;
    margin-left: auto;
    margin-right: 0;
    margin-top: 0;
